    <script type="text/javascript">
        var item_to_delete;
     
        $(".deleteItem").click(function (e) {
            item_to_delete = $(this).data('id');
        });
        $('#btnContinueDelete').click(function () {
            window.location = "/Organization/removeOrganization/" + item_to_delete;
        });


        $(".positiondeleteItem").click(function (e) {
            item_to_delete = $(this).data('id');
        });
        $('#positionbtnContinueDelete').click(function () {
            window.location = "/Position/removePosition/" + item_to_delete;
        });

        $(".departmentdeleteItem").click(function (e) {
            item_to_delete = $(this).data('id');
        });
        $('#departmentbtnContinueDelete').click(function () {
            window.location = "/Department/removeDepartment/" + item_to_delete;
        });

        $(".BiogDepartmentPositiondeleteItem").click(function (e) {
            item_to_delete = $(this).data('id');
        });
        $('#BiogDepartmentPositionDelete').click(function () {
            window.location = "/BioDepPosition/removDepPos/" + item_to_delete;
